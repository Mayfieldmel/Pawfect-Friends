<h4>Upload Image</h4>
<form>
<div class="form-group">
    <input
    type="file"
    name="file"
    id="input-files"
    action="/upload"
    method="POST"
    enctype="multipart/form-data"
    />
</div>
<button type="submit" id="submitImgBtn">Submit</button>
</form>

<div id="preview-images"></div>

<script src="/javascript/image.js"></script>
